<template name="TextOutput" element="pages" type="Processing" >
  <group size="760 260"
    style="Heading1"
    label="Text Output"
    bg=""
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="no"
    expanded="yes"
    >
    <button loc="630 220" size="80 20"
      style="Heading2"
      label="Clear"
      vc="clear"
      decor1=""
      />
    <button loc="630 190" size="80 20"
      style="Heading2"
      label="More"
      vc="dgmore"
      decor1=""
      />
    <button loc="630 160" size="80 20"
      style="Heading2"
      label="Seq Manual"
      vc="man(seqfil)"
      decor1=""
      />
    <button loc="630 130" size="80 20"
      style="Heading2"
      label="Shims"
      vc="dgs"
      decor1=""
      />
    <button loc="630 100" size="80 20"
      style="Heading2"
      label="Channels"
      vc="dg2"
      decor1=""
      />
    <button loc="630 70" size="80 20"
      style="Heading2"
      label="Array"
      vc="da"
      decor1=""
      />
    <button loc="630 40" size="80 20"
      style="Heading2"
      label="Basic"
      vc="dg"
      decor1=""
      />
    <button loc="630 10" size="80 20"
      style="Heading2"
      label="Text"
      vc="clear text"
      decor1=""
      />
    <textfile loc="10 0" size="600 260"
      style="PlainText"
      vq="alphatext"
      set="alphatextname:$VALUE"
      editable="no"
      wrap="yes"
      />
  </group>
</template>
