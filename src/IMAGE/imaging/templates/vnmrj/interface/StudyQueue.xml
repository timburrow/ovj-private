<template element="panel" type="system" name="StudyQueue" >
  <group loc="5 5" size="290 30"
    style="Plain"
    border="Etched"
    side="Top"
    justify="Left"
    tab="false"
    >
    <selmenu loc="10 5" size="120 20"
      style="PlainText"
      label="Study Options"
      vc="sqfilemenu('$VALUE')"
      >
        <mlabel
          label="New Study"
          chval="NewStudy"
          />
        <mlabel
          label="Continue Study"
          chval="ContinueStudy"
          />
      </selmenu>

    <button loc="160 5" size="120 20"
      style="Label4"
      label="Start Queue"
      vq="activestudy"
      bg="0x006400"
      vc="xmq('start')"
      show="$SHOW=(activestudy&lt;&gt;'null' and size('activestudy')&lt;1.5)*2-1"
      decor1="yes"
      />
  </group>
</template>
