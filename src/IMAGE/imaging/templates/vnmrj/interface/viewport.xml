<template element="panels" type="acquisition" >
  <toolBarButton loc="0 5" size="71 25" style="Plain"
    vq="jviewports jviewportlabel"
    vc="M@vplayout1"
    set="$VALUE=jviewportlabel[1]"
    enabled="yes"
    icon="Plan"
    show="if jviewports[1]&gt;1 then $VALUE=1 else $VALUE=0 endif"
    subtype="vp1"
    toolTip="Plan"
    />
  <toolBarButton loc="71 5" size="77 25" style="Plain"
    vq="jviewports jviewportlabel"
    vc="M@vplayout2"
    set="$VALUE=jviewportlabel[2]"
    enabled="yes"
    icon="Current"
    show="if jviewports[1]&gt;=2 then $VALUE=1 else $VALUE=0 endif"
    subtype="vp2"
    toolTip="Current"
    />
  <toolBarButton loc="148 5" size="59 25" style="Plain"
    vq="jviewports jviewportlabel"
    vc="M@vplayout3"
    set="$VALUE=jviewportlabel[3]"
    enabled="yes"
    icon="Review"
    show="if jviewports[1]&gt;=3 then $VALUE=1 else $VALUE=0 endif"
    subtype="vp3"
    toolTip="Review"
    />
</template>
