<actionpanel>
  <group size="700 20" fg="PlainText" bg="transparent" border="None"
    side="Top" justify="Left" tab="no" reference="aip_action" useref="no"
    >
    <label loc="0 0" size="290 20"
      label=""
      />
    <button loc="0 0" size="100 20"
      style="PlainText"
      label=" Cancel "
      vc="M@cancel"
      />
    <button loc="600 0" size="60 20"
      style="PlainText"
      fg="white" bg="red"
      label="Stop"
      vq="system"
      vc="sqmode='pause' aa"
      show="$VALUE=2*(system='spectrometer')-1"
      />
  </group>
</actionpanel>
