        <Date value="datetrashed" display="false"/>
        <Calendar value="on any date"/>
        <UserType value="owner$me$exclusive" display="false"/>
        <Columns value="filename$objtype$datetrashed"/>
        <Sort  value="objtype$ASC"/>
    </Statement>

</shuffler>
