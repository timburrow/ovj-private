<template name="deleteRec" element="pages" type="processing" >
    <group loc="0 0" size="500 40" style="Plain"
      point="8"
      fg="blue"
      tab="false"
      >
      <entry loc="8 48" size="0 0" font="Dialog"
        style="Heading2"
        point="Heading2"
        fg="black"
	vc="deleteREC"
	set="$VALUE=''"
	show="$SHOW=-1"
        />
      <label loc="8 8" size="490 30" font="Dialog"
        style="Heading2"
        point="Heading2"
        fg="Label1"
	label="Click ok will delete data linked to current exp."
        />
    </group>
</template>
